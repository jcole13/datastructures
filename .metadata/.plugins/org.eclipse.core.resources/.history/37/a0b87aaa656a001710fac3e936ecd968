package main;

public class Test {
	
	public static void main(String [] args) {
		
		Graph g = new Graph();
		GraphNode A = new GraphNode(new People("Stan"));
		GraphNode B = new GraphNode(new People("Jerry"));
		GraphNode C = new GraphNode(new People("Richard"));
		g.addNode(A);
		g.addNode(B);
		g.addNode(C);
		g.addEdge(A, B);
		g.adjList.adjfind(B);
		
	}

}
